<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ccsu.feng.test.dao.UserBasesMapper">


    <insert id="insertAndGetId"  parameterType="com.ccsu.feng.test.entity.UserBases">
        <selectKey resultType="java.lang.Integer" order="AFTER" keyProperty="id">
            SELECT LAST_INSERT_ID()
        </selectKey>
      insert into user_bases (nick_name,picture) values(#{nickName},#{picture})

    </insert>
</mapper>